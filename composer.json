{
    "name": "blue32a/laravel-language-ja",
    "description": "Japanese language resources for Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "blue32a",
            "email": "blue32a.jp@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3",
        "blue32a/php-coding-standard": "v0.1.0-alpha"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blue32a\\Laravel\\Language\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Blue32a\\Laravel\\Language\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "cs": "vendor/bin/phpcs",
        "cbf": "vendor/bin/phpcbf",
        "test": "vendor/bin/phpunit"
    }
}
